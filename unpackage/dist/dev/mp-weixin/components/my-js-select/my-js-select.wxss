.arrivalNavigation {
  width: 210rpx;
  position: absolute;
  left: 20rpx;
  z-index: 9999;
}
.arrivalNavigation .sideNavigation {
  width: 208rpx;
  background-color: #202020;
  color: #eee;
  border-radius: 10rpx;
}
.arrivalNavigation .sideNavigation .li {
  height: 85rpx;
  text-align: center;
  line-height: 85rpx;
  font-size: 25rpx;
}
.arrivalNavigation .sideNavigation .liBottomBorder {
  border: 0.1rpx solid #373737;
}
.arrivalNavigation .d4 {
  width: 0;
  height: 0;
  margin-left: 20rpx;
  margin-top: -20rpx;
  border-width: 20rpx;
  border-style: solid;
  border-color: transparent #373737 transparent transparent;
  transform: rotate(90deg);
  /*顺时针旋转90°*/
}
